{
  "name": "FinanceAppIgnite",
  "displayName": "FinanceAppIgnite",
  "expo": {
    "name": "FinanceAppIgnite",
    "slug": "FinanceAppIgnite",
    "scheme": "financeappignite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/mercury.png",
    "splash": {
      "image": "./assets/logo/mercury.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "icon": "./assets/logo/mercury.png",
      "package": "com.financeappignite",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/mercury.png",
        "backgroundImage": "./assets/logo/mercury.png"
      },
      "splash": {
        "image": "./assets/logo/mercury.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffff"
      }
    },
    "ios": {
      "icon": "./assets/logo/mercury.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.financeappignite",
      "splash": {
        "image": "./assets/logo/mercury.png",
        "tabletImage": "./assets/logo/mercury.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffff"
      }
    },
    "web": {
      "favicon": "./assets/logo/mercury.png",
      "splash": {
        "image": "./assets/logo/mercury.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffff"
      }
    }
  }
}
